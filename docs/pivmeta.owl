    <!-- https://codemeta.github.io/terms/hasSourceCode -->


    <owl:ObjectProperty rdf:about="https://codemeta.github.io/terms/hasSourceCode">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/pivmeta#DigitalCameraModel"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/pivmeta#LaserModel"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/pivmeta#SyntheticParticle"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://schema.org/SoftwareSourceCode"/>
        <skos:prefLabel xml:lang="en">source code</skos:prefLabel>
    </owl:ObjectProperty>
    


