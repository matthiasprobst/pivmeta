    <owl:Ontology rdf:about="https://matthiasprobst.github.io/pivmeta#">
        <terms:abstract xml:lang="en">This is an ontology draft for the description of Particle Image Velocimetry data. It is currently limited to planar PIV (2D2C and 2D3C) setups.</terms:abstract>
        <terms:created>2024-01-03</terms:created>
        <terms:creator>Matthias Probst (https://orcid.org/0000-0001-8729-0482), Institut für Thermische Strömungsmaschinen</terms:creator>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:modified>2024-01-03</terms:modified>
        <terms:title xml:lang="en">pivmeta: An ontology draft for describing particle image velocimetry data.</terms:title>
        <vann:preferredNamespacePrefix>pivmeta</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://matthiasprobst.github.io/pivmeta#</vann:preferredNamespaceUri>
        <metadata4ing:ContactPerson>https://orcid.org/0000-0001-8729-0482</metadata4ing:ContactPerson>
        <rdfs:label>pivmeta</rdfs:label>
        <owl:versionInfo>v1.0.0</owl:versionInfo>
