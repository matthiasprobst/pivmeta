    <!-- http://w3id.org/nfdi4ing/metadata4ing#hasParameter -->


    <owl:ObjectProperty rdf:about="http://w3id.org/nfdi4ing/metadata4ing#hasParameter">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://w3id.org/nfdi4ing/metadata4ing#Method"/>
                    <rdf:Description rdf:about="http://w3id.org/nfdi4ing/metadata4ing#Tool"/>
                    <rdf:Description rdf:about="https://matthiasprobst.github.io/pivmeta#VirtualTool"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Variable"/>
        <skos:definition xml:lang="en">Points to a parameter of a given method or tool.</skos:definition>
        <skos:prefLabel xml:lang="en">has parameter</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://codemeta.github.io/terms/hasSourceCode -->


    <owl:ObjectProperty rdf:about="https://codemeta.github.io/terms/hasSourceCode">
        <owl:inverseOf rdf:resource="https://codemeta.github.io/terms/isSourceCodeOf"/>
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/pivmeta#VirtualTool"/>
        <rdfs:range rdf:resource="https://schema.org/SoftwareSourceCode"/>
        <rdfs:isDefinedBy rdf:resource="https://codemeta.github.io/terms/"/>
        <skos:definition xml:lang="en">Relates a computational model or concept to the source code that implements it.</skos:definition>
        <skos:prefLabel xml:lang="en">has source code</skos:prefLabel>
    </owl:ObjectProperty>
    


