###  https://matthiasprobst.github.io/pivmeta#hasStandardName
:hasStandardName rdf:type owl:ObjectProperty ;
                 rdfs:domain <http://w3id.org/nfdi4ing/metadata4ing#NumericalVariable> ;
                 rdfs:range :StandardName ;
                 <http://www.w3.org/2004/02/skos/core#prefLabel> "has standard name"@en .


###  https://matthiasprobst.github.io/pivmeta#pivImageType
:pivImageType rdf:type owl:ObjectProperty ;
              rdfs:domain :PivImageDistribution ;
              rdfs:range :ImageType ;
              <http://www.w3.org/2004/02/skos/core#prefLabel> "piv image type"@en .


###  https://matthiasprobst.github.io/pivmeta#usesCorrelationAlgorithm
:usesCorrelationAlgorithm rdf:type owl:ObjectProperty ;
                          rdfs:domain :InterrogationMethod ;
                          rdfs:range :CorrelationAlgorithm ;
                          <http://www.w3.org/2004/02/skos/core#prefLabel> "correlation algorithm"@en .


###  https://matthiasprobst.github.io/ssno#unit
<https://matthiasprobst.github.io/ssno#unit> rdf:type owl:ObjectProperty ;
                                             rdfs:domain :StandardName ;
                                             rdfs:range <http://qudt.org/schema/qudt/Unit> ;
                                             rdfs:seeAlso <http://w3id.org/nfdi4ing/metadata4ing#hasUnit> ;
                                             <http://www.w3.org/2004/02/skos/core#definition> "The canonical unit of the standard name"@en ;
                                             <http://www.w3.org/2004/02/skos/core#prefLabel> "canonical units"@en .


