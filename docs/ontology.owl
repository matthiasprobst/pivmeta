    <!-- https://matthiasprobst.github.io/pivmeta#estimated_particle_image_density -->


    <owl:NamedIndividual rdf:about="https://matthiasprobst.github.io/pivmeta#estimated_particle_image_density">
        <rdf:type rdf:resource="https://matthiasprobst.github.io/ssno#StandardName"/>
        <skos:definition xml:lang="en">The number of particles per pixel (ppp) estimated by means of an algorithm or by manual counting the particles.</skos:definition>
