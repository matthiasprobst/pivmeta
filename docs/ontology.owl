    <!-- https://matthiasprobst.github.io/pivmeta#inCoordinateSystem -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/pivmeta#inCoordinateSystem">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#NumericalVariable"/>
        <rdfs:range rdf:resource="http://semanticscience.org/resource/SIO_000320"/>
        <skos:definition xml:lang="en">A coordinate system in which the numeric variable is defined.</skos:definition>
        <skos:example xml:lang="en">Velocity data can be defined in a moving coordinate system and will therefore have different values to a stationary system.</skos:example>
        <skos:prefLabel xml:lang="en">in coordinate system</skos:prefLabel>
    </owl:ObjectProperty>
    


