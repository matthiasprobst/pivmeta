        <rdfs:range rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#NumericalVariable"/>
        <skos:definition xml:lang="en">Points to a velocity of a given coordinate system.</skos:definition>
        <skos:definition xml:lang="de">hat Geschwindigkeit</skos:definition>
        <skos:prefLabel xml:lang="en">has velocity</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/pivmeta#has_origin -->


