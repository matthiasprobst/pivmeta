    <!-- https://matthiasprobst.github.io/pivmeta#hasEstimatedParticleImageDensity -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/pivmeta#hasEstimatedParticleImageDensity">
        <rdfs:subPropertyOf rdf:resource="https://matthiasprobst.github.io/pivmeta#hasParticleImageDensity"/>
        <skos:definition xml:lang="en">The number of particles per pixel estimated by means of an algorithm or by manual counting the particles.</skos:definition>
        <skos:prefLabel xml:lang="en">estimated particle image density</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/pivmeta#hasMeasuredParticleImageDensity -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/pivmeta#hasMeasuredParticleImageDensity">
        <rdfs:subPropertyOf rdf:resource="https://matthiasprobst.github.io/pivmeta#hasParticleImageDensity"/>
        <skos:definition xml:lang="en">The number of particles per pixel measured by a measurement device.</skos:definition>
        <skos:prefLabel xml:lang="en">measured particle image density</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/pivmeta#hasParticleImageDensity -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/pivmeta#hasParticleImageDensity">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/pivmeta#PIVProcessingStep"/>
        <rdfs:range rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#NumericalVariable"/>
        <skos:definition xml:lang="en">The number of particles per pixel is an important parameter for the performance of a PIV algorithm. It also allows a distinction between different PIV methods such as PTV, PIV or speckle PIV. It may be measured experimentally with a particle counter or estimated by a numerical or manual (counting) method.</skos:definition>
        <skos:prefLabel xml:lang="en">particle image density</skos:prefLabel>
    </owl:ObjectProperty>
    


    <!-- https://matthiasprobst.github.io/pivmeta#hasStandardName -->


    <owl:ObjectProperty rdf:about="https://matthiasprobst.github.io/pivmeta#hasStandardName">
        <owl:inverseOf rdf:resource="https://matthiasprobst.github.io/pivmeta#partOfStandardNameTable"/>
        <rdfs:domain rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#NumericalVariable"/>
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/pivmeta#StandardNameTable"/>
        <rdfs:range rdf:resource="http://mmisw.org/ont/cf/parameter/Standard_Name"/>
        <rdfs:seeAlso rdf:resource="https://w3id.org/okn/o/sd#hasStandardVariable"/>
        <skos:prefLabel xml:lang="en">has standard name</skos:prefLabel>
    </owl:ObjectProperty>
    


