    <!-- https://matthiasprobst.github.io/pivmeta#x_displacement -->


    <owl:NamedIndividual rdf:about="https://matthiasprobst.github.io/pivmeta#x_displacement">
        <skos:definition xml:lang="en">Approximated displacement of tracer particles determined by a PIV algorithm. Displacement is a vector quantity. X indicates the component in x-axis direction of the respective coordinate system.</skos:definition>
        <skos:prefLabel xml:lang="en">x displacement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- https://matthiasprobst.github.io/pivmeta#x_true_particle_displacement -->


    <owl:NamedIndividual rdf:about="https://matthiasprobst.github.io/pivmeta#x_true_particle_displacement">
        <rdfs:comment xml:lang="en">Note, that tracer particle displacement is generally only known from synthetically generated PIV images.</rdfs:comment>
        <skos:definition xml:lang="en">True displacement of tracer particles. Displacement is a vector quantity. X indicates the component in x-axis direction of the respective coordinate system.</skos:definition>
        <skos:prefLabel xml:lang="en">x true particle displacement</skos:prefLabel>
    </owl:NamedIndividual>
    


    <!-- https://matthiasprobst.github.io/pivmeta#x_velocity -->


    <owl:NamedIndividual rdf:about="https://matthiasprobst.github.io/pivmeta#x_velocity">
        <skos:definition xml:lang="en">Approximated velocity of medium determined by a PIV algorithm based on tracer movement. Velocity is a vector quantity. X indicates the component in x-axis direction of the respective coordinate system.</skos:definition>
        <skos:prefLabel xml:lang="en">x velocity</skos:prefLabel>
    </owl:NamedIndividual>
    


