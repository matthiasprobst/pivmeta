        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <skos:definition xml:lang="en">Description of a standard name or a standard name table</skos:definition>
        <skos:prefLabel xml:lang="en">standard name description</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- https://matthiasprobst.github.io/ssno#standard_name -->


