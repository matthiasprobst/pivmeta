        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
        <skos:definition xml:lang="en">The pixel depth of an image.</skos:definition>
        <skos:prefLabel xml:lang="en">image bit depth</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- https://matthiasprobst.github.io/pivmeta#numberOfRecords -->


    <owl:DatatypeProperty rdf:about="https://matthiasprobst.github.io/pivmeta#numberOfRecords">
        <rdfs:domain rdf:resource="https://matthiasprobst.github.io/pivmeta#PivImageDistribution"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
        <skos:definition xml:lang="en">The number of records. A record describes the number of images needed for the evaluation of a single PIV velocity field. E.g. a double image recording stored in two separate files A1.tiff and B1.tiff counts as one record. Hence, the number of records correspond to the number of velocity fields, if all records are taken into account in the evluation process.</skos:definition>
        <skos:prefLabel xml:lang="en">number of records</skos:prefLabel>
    </owl:DatatypeProperty>
    


