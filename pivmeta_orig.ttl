    rdfs:domain    pivmeta:PivImageDistribution ;
    rdfs:range     pivmeta:ImageType ;
    skos:definition
                   "The type of image, e.g. an eperimental or synthetic one. Helps to identify the image source and hence assists processing or analysing PIV data."@en ;
    skos:example   "Individuals of type ImageType are provided by the ontology, e.g. pivmeta:ExperimentalImage"@en ;
    skos:prefLabel "piv image type"@en .


###  https://matthiasprobst.github.io/pivmeta#windowWeightingFunction
pivmeta:windowWeightingFunction
    rdf:type        owl:ObjectProperty ;
    rdfs:domain     pivmeta:CorrelationMethod ;
    rdfs:range      pivmeta:WindowWeightingFunction ;
    skos:definition "Points to the window weighting function used by the correlation method."@en ;
    skos:prefLabel  "window weighting function"@en .


