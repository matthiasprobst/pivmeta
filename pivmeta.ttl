###  http://purl.obolibrary.org/obo/BFO_0000050
obo:BFO_0000050 rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                skos:definition "a core relation that holds between a part and its whole"@en ;
                skos:prefLabel "part of"@en .


###  http://purl.obolibrary.org/obo/BFO_0000051
obo:BFO_0000051 rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                skos:definition "a core relation that holds between a whole and its part"@en ;
                skos:prefLabel "has part"@en .


###  http://purl.obolibrary.org/obo/BFO_0000063
obo:BFO_0000063 rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                rdfs:domain obo:Process ;
                rdfs:range obo:Process ;
                skos:definition "x precedes y if and only if the time point at which x ends is before or equivalent to the time point at which y starts. Formally: x precedes y iff ω(x) <= α(y), where α is a function that maps a process to a start point, and ω is a function that maps a process to an end point."@en ;
                skos:prefLabel "precedes"@en .


###  http://purl.obolibrary.org/obo/RO_0002090
obo:RO_0002090 rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf obo:BFO_0000063 ;
               rdfs:domain obo:Process ;
               rdfs:range obo:Process ;
               skos:definition "X immediately precedes_Y iff: end(X) simultaneous_with start(Y)"@en ;
               skos:prefLabel "immediately precedes"@en .


###  http://purl.org/dc/terms/type
dcterms:type rdf:type owl:ObjectProperty ;
             skos:prefLabel "type"@en .


###  http://w3id.org/nfdi4ing/metadata4ing#hasEmployedTool
m4i:hasEmployedTool rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf schema:instrument ;
                    owl:inverseOf m4i:isEmployedToolIn ;
                    rdfs:domain m4i:ProcessingStep ;
                    rdfs:range m4i:Tool ;
                    skos:definition "Points to a tool that is used within a given processing step"@en ;
                    skos:prefLabel "has employed tool"@en .


###  http://w3id.org/nfdi4ing/metadata4ing#hasParameter
m4i:hasParameter rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( m4i:Method
                                             m4i:Tool
                                             piv:VirtualTool
                                           )
                             ] ;
                 rdfs:range pims-ii:Variable ;
                 skos:definition "Points to a parameter of a given method or tool."@en ;
                 skos:prefLabel "has parameter"@en .


###  http://w3id.org/nfdi4ing/metadata4ing#isEmployedToolIn
m4i:isEmployedToolIn rdf:type owl:ObjectProperty ;
                     skos:definition "Points to a processing step the tool has been used in"@en ;
                     skos:prefLabel "is employed tool"@en .


###  http://w3id.org/nfdi4ing/metadata4ing#realizesMethod
m4i:realizesMethod rdf:type owl:ObjectProperty ;
                   rdfs:domain m4i:ProcessingStep ;
                   rdfs:range m4i:Method ;
                   skos:definition "Points to a method that is used within a given processing step"@en ;
                   skos:prefLabel "realizes method" .


###  http://www.w3.org/ns/dcat#distribution
dcat:distribution rdf:type owl:ObjectProperty ;
                  rdfs:domain dcat:Dataset ;
                  rdfs:range dcat:Distribution ;
                  skos:definition "An available distribution of the dataset."@en ;
                  skos:prefLabel "distribution"@en .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty ;
                    rdfs:domain dcat:Dataset ;
                    rdfs:range prov:Activity ;
                    skos:prefLabel "was generated by"@en .


###  https://codemeta.github.io/terms/hasSourceCode
codemeta:hasSourceCode rdf:type owl:ObjectProperty ;
                       owl:inverseOf codemeta:isSourceCodeOf ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( piv:VirtualPIVSetup
                                                   piv:VirtualTool
                                                 )
                                   ] ;
                       rdfs:range schema:SoftwareSourceCode ;
                       rdfs:isDefinedBy codemeta: ;
                       skos:definition "Relates a computational model or concept to the source code that implements it."@en ;
                       skos:prefLabel "has source code"@en .


###  https://codemeta.github.io/terms/isSourceCodeOf
codemeta:isSourceCodeOf rdf:type owl:ObjectProperty ;
                        rdfs:domain schema:SoftwareSourceCode ;
                        rdfs:range [ rdf:type owl:Class ;
                                     owl:unionOf ( piv:VirtualPIVSetup
                                                   piv:VirtualTool
                                                 )
                                   ] ;
                        rdfs:isDefinedBy codemeta: ;
                        skos:prefLabel "is source code of"@en .


###  https://matthiasprobst.github.io/pivmeta#flag
piv:flag rdf:type owl:ObjectProperty ;
         owl:inverseOf piv:flagIn ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( <http://purl.allotrope.org/ontologies/hdf5/1.8#Dataset>
                                     piv:FlagStatistics
                                   )
                     ] ;
         rdfs:range piv:FlagVariable ;
         skos:definition "Relates something to a Flag Variable"@en ;
         skos:prefLabel "flag"@en .


###  https://matthiasprobst.github.io/pivmeta#flagIn
piv:flagIn rdf:type owl:ObjectProperty ;
           skos:definition "Relates a Flag Variable to its used concept"@en ;
           skos:prefLabel "flag in"@en .


###  https://matthiasprobst.github.io/pivmeta#hasMetric
piv:hasMetric rdf:type owl:ObjectProperty ;
              rdfs:domain piv:VelocimetryDistribution ;
              rdfs:range pims-ii:Variable ;
              skos:definition "A numerical value derived from or related to the distribution's content."@en ;
              skos:prefLabel "has metric"@en .


###  https://matthiasprobst.github.io/pivmeta#hasPIVDataType
piv:hasPIVDataType rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf dcterms:type ;
                   rdfs:domain piv:VelocimetryDistribution ;
                   rdfs:range piv:PIVDataType ;
                   skos:definition "relates the PIV data type to a distribution"@en ;
                   skos:prefLabel "has PIV data type"@en .


###  https://matthiasprobst.github.io/pivmeta#hasSetup
piv:hasSetup rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf obo:BFO_0000051 ;
             owl:inverseOf piv:isSetupFor ;
             rdfs:domain piv:VelocimetryDataset ;
             rdfs:range piv:Setup ;
             skos:definition "Relates a dataset to the setup used to generate it."@en ;
             skos:prefLabel "has setup"@en .


###  https://matthiasprobst.github.io/pivmeta#hasWindowWeightingFunction
piv:hasWindowWeightingFunction rdf:type owl:ObjectProperty ;
                               rdfs:domain piv:CorrelationMethod ;
                               rdfs:range piv:WindowWeightingFunction ;
                               skos:definition "Points to the window weighting function used by the correlation method."@en ;
                               skos:prefLabel "has window weighting function"@en .


###  https://matthiasprobst.github.io/pivmeta#isSetupFor
piv:isSetupFor rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf obo:BFO_0000050 ;
               rdfs:domain piv:Setup ;
               rdfs:range piv:VelocimetryDataset ;
               skos:definition "Relates a setup to the dataset(s) it was used for."@en ;
               skos:prefLabel "is setup for"@en .


###  https://matthiasprobst.github.io/pivmeta#manufacturer
piv:manufacturer rdf:type owl:ObjectProperty ;
                 rdfs:domain m4i:Tool ;
                 rdfs:range prov:Organization ;
                 skos:closeMatch schema:manufacturer ;
                 skos:definition "Manufacturer of a m4i:Tool"@en ;
                 skos:prefLabel "manufacturer"@en .


###  https://matthiasprobst.github.io/pivmeta#outlierReplacementScheme
piv:outlierReplacementScheme rdf:type owl:ObjectProperty ;
                             rdfs:range piv:OutlierReplacementScheme ;
                             skos:definition "Points to the outlier replacement scheme used by the PIV validation process."@en ;
                             skos:prefLabel "outlier replacement scheme"@en .


###  https://matthiasprobst.github.io/ssno#hasStandardName
ssno:hasStandardName rdf:type owl:ObjectProperty ;
                     rdfs:domain m4i:NumericalVariable ;
                     rdfs:range ssno:StandardName ;
                     skos:prefLabel "has standard name"@en .


###  https://matthiasprobst.github.io/ssno#unit
ssno:unit rdf:type owl:ObjectProperty ;
          rdfs:domain ssno:StandardName ;
          rdfs:range qudt:Unit ;
          rdfs:seeAlso m4i:hasUnit ;
          skos:definition "The canonical unit of the standard name"@en ;
          skos:prefLabel "unit"@en .


###  https://schema.org/instrument
schema:instrument rdf:type owl:ObjectProperty ;
                  rdfs:domain prov:Activity ;
                  skos:definition "points to an object that helped the agent perform the action (e.g., John wrote a book with a pen)"@en ;
                  skos:prefLabel "uses instrument"@en .


